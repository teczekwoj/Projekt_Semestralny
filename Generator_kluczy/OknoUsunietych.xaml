<Window x:Class="Generator_kluczy.OknoUsunietych"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Generator_kluczy"
        mc:Ignorable="d"
        Title="Usuniete..." Height="450" Width="800">
    <ListView Name="UILista" d:ItemsSource="{d:SampleData ItemCount=5}">
        <ListView.View>
            <GridView>
                <GridViewColumn Header="Klucz" DisplayMemberBinding="{Binding Klucz}" />
                <GridViewColumn Header="Produkt" DisplayMemberBinding="{Binding Produkt}" />
                <GridViewColumn Header="Status" DisplayMemberBinding="{Binding Status}" />
            </GridView>
        </ListView.View>
    </ListView>
</Window>
